{
  "ConnectionStrings": {
    "DefaultConnection": "app.db"
  },
  "DatabasePath": {
    "Local": "Data/",
    "Docker": "/app/data/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:8444"
      },
      "Http": {
        "Url": "http://*:8081"
      }
    }
  },
  "AllowedHosts": "*"
}
