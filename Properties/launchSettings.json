{
  "profiles": {
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/index.html",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://+:8444;http://+:8081",
        "ASPNETCORE_HTTPS_PORT": "8444",
        "ASPNETCORE_HTTP_PORT": "8081"
      },
      "publishAllPorts": true,
      "useSSL": true,
      "httpPort": 8081,
      "sslPort": 8444
    },
    "WSL": {
      "commandName": "WSL2",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HTTPS_PORT": "8444",
        "ASPNETCORE_HTTP_PORT": "8081",
        "ASPNETCORE_URLS": "https://+:8444;http://+:8081"
      },
      "distributionName": ""
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json"
}
